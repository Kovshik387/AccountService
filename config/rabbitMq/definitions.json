  "exchanges": [
    {
      "name": "account.events",
      "vhost": "default",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "account.crm",
      "vhost": "default",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "account.notifications",
      "vhost": "default",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "account.antifraud",
      "vhost": "default",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "account.audit",
      "vhost": "default",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "account.events",
      "vhost": "default",
      "destination": "account.crm",
      "destination_type": "queue",
