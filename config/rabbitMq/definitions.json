  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    },
    {
      "name": "user",
      "password": "user",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "user",
      "vhost": "default",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
